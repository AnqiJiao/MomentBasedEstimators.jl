## New test

srand(1)
using ModelsGenerators
y,x,z=randiv()
xb = randn(100,2)
x = [x xb]
z = [z xb]

f(theta) = z.*(y-x*theta);

g = MomentBasedEstimators.GMMEstimator(f, [.1, 0, 0])
m = MomentBasedEstimators.MDEstimator(f, [.1, 0, 0])

MomentBasedEstimators.equality_constraint!(g, h)
MomentBasedEstimators.equality_constraint!(m, h)

MomentBasedEstimators.inequality_constraint!(g, h)
MomentBasedEstimators.inequality_constraint!(m, h)





